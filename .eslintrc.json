{
	"root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
	"plugins": [
		"babel"
	],
	"parser": "babel-eslint",
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
		"globalReturn": true
    },
    "rules": {
		"no-console": "off",
		"quotes": [
			"error",
			"single"
		],
    }
}
